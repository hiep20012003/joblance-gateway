{
  "name": "gateway-service",
  "version": "0.0.1",
  "main": "app.js",
  "scripts": {
    "start": "dotenv -e .env.prod -- pm2 start ./build/src/app.js -i 5 --attach --watch",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "dev": "dotenv -e .env.dev -- nodemon -r tsconfig-paths/register src/app.ts",
    "link": "npm link @hiep20012003/joblance-shared",
    "lint:check": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier:check": "prettier --check src/**/*.{ts,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,json}",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "test": "jest --coverage --maxWorkers=1 --forceExit --detectOpenHandles --watchAll=false"
  },
  "dependencies": {
    "@elastic/opentelemetry-node": "^1.6.0",
    "@hiep20012003/joblance-shared": "latest",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv-cli": "^8.0.0",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "install": "^0.13.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "multer": "^2.0.2",
    "npm": "^11.6.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.19",
    "@types/estree": "^1.0.7",
    "@types/express": "^5.0.3",
    "@types/form-data": "^2.2.1",
    "@types/hpp": "^0.2.6",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.16.3",
    "@types/pem-jwk": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "typescript-transform-paths": "^3.5.3"
  }
}
